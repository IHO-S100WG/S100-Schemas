	<!-- Restrictions of standard types, used to implement generic constraints in Part 17 -->
	<xs:simpleType name="URNType">
		<xs:annotation><xs:documentation>Generic type for URN</xs:documentation></xs:annotation>
		<xs:restriction  base="xs:anyURI">
			<xs:pattern value="urn:.+"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="MRNType">
		<xs:annotation><xs:documentation>Generic type for MRN</xs:documentation></xs:annotation>
		<xs:restriction  base="xs:anyURI">
			<xs:pattern value="urn:mrn:.+"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="RestrictedBoolean">
		<xs:annotation><xs:documentation>S-100 5.0 metadata requires booleans to be true/false, not 1/0</xs:documentation></xs:annotation>
		<xs:restriction base="xs:boolean">
			<xs:pattern value="(true)|(false)"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="S100_Purpose">
		<xs:annotation>
			<xs:documentation>The purpose of the dataset</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="newDataset">
				<xs:annotation>
					<xs:documentation>Brand new dataset</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="newEdition">
				<xs:annotation>
					<xs:documentation>New edition of the dataset or Catalogue</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="update">
				<xs:annotation>
					<xs:documentation>Dataset update</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="reissue">
				<xs:annotation>
					<xs:documentation>Dataset that has been re-issued</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="cancellation">
				<xs:annotation>
					<xs:documentation>Dataset or Catalogue that has been cancelled</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="delta">
				<xs:annotation>
					<xs:documentation>Dataset difference</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="S100_NavigationPurpose">
		<xs:annotation>
			<xs:documentation>The navigational purpose of the dataset</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="port">
				<xs:annotation>
					<xs:documentation>For port and near shore operations</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="transit">
				<xs:annotation>
					<xs:documentation>For coast and planning purposes</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="overview">
				<xs:annotation>
					<xs:documentation>For ocean crossing and planning purposes</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="S100_CompliancyCategory">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="category1">
				<xs:annotation>
					<xs:documentation>IHO S-100 object model compliant</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="category2">
				<xs:annotation>
					<xs:documentation>IHO S-100 compliant with non-standard encoding</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="category3">
				<xs:annotation>
					<xs:documentation>IHO S-100 compliant with standard encoding</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="category4">
				<xs:annotation>
					<xs:documentation>IHO S-100 and IMO harmonized display compliant</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="S100_SupportFileRevisionStatus">
		<xs:annotation>
			<xs:documentation>The reason for inclusion of the support file in this Exchange Set</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="new">
				<xs:annotation>
					<xs:documentation>A file which is new</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="replacement">
				<xs:annotation>
					<xs:documentation>A file which replaces an existing file</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="deletion">
				<xs:annotation>
					<xs:documentation>Deletes an existing file</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="S100_ResourcePurpose">
		<xs:annotation>
			<xs:documentation>Defines the purpose of the supporting resource</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="dataset">
				<xs:annotation>
					<xs:documentation>A dataset</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="featureCatalogue">
				<xs:annotation>
					<xs:documentation>A Feature Catalogue for an S-100 data product</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="portrayalCatalogue">
				<xs:annotation>
					<xs:documentation>A Portrayal Catalogue for an S-100 data product</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="interoperabilityCatalogue">
				<xs:annotation>
					<xs:documentation>An Interoperability Catalogue</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="supportFile">
				<xs:annotation>
					<xs:documentation>A support file</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="productVersion">
				<xs:annotation>
					<xs:documentation>All datasets conforming to a specific version of an S-100 Product Specification</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="productFamily">
				<xs:annotation>
					<xs:documentation>All datasets conforming to any active version of an S-100 Product Specification</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="software">
				<xs:annotation>
					<xs:documentation>Application software</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="system">
				<xs:annotation>
					<xs:documentation>Provides support or common information for a variety of applications and products</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="exchangeCatalogue">
				<xs:annotation>
					<xs:documentation>An Exchange Catalogue</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ISOMetadata">
				<xs:annotation>
					<xs:documentation>Dataset metadata in ISO format</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="languagePack">
				<xs:annotation>
					<xs:documentation>A Language pack</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GMLSchema">
				<xs:annotation>
					<xs:documentation>GML Application Schema</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="other">
				<xs:annotation>
					<xs:documentation>A type of resource not otherwise described</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="S100_ProductSpecification" type="S100_ProductSpecification"/>
	<xs:complexType name="S100_ProductSpecification">
		<xs:annotation>
			<xs:documentation>The Product Specification contains the information needed to build the specified product</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The name of the product specification used to create the datasets.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version number of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="xs:date" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version date of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productIdentifier" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Machine readable unique identifier of a product type. For example, “S-101”</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="number" type="xs:nonNegativeInteger" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The number used to lookup the product in the Product Specification Register of the IHO GI registry</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="compliancyCategory" type="S100_CompliancyCategory" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The level of compliance of the Product Specification to S-100</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="S100_SupportFileSpecification" type="S100_SupportFileSpecification"/>
	<xs:complexType name="S100_SupportFileSpecification">
		<xs:annotation>
			<xs:documentation>The Support File Specification contains the information needed to build the specified product</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The name of the specification used to create the support file.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version number of the specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="xs:date" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version date of the specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="S100_SupportFilePurpose">
		<xs:annotation>
			<xs:documentation>The reason for inclusion of the support file in this exchange set</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="new">
				<xs:annotation>
					<xs:documentation>A file which is new. Signifies a new file.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="replacement">
				<xs:annotation>
					<xs:documentation>A file which replaces an existing file. Signifies a replacement for a file of the same name.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="deletion">
				<xs:annotation>
					<xs:documentation>Deletes an existing file. Signifies deletion of a file of that name.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="S100_SupportFileDiscoveryMetadata" type="S100_SupportFileDiscoveryMetadata"/>
	<xs:complexType name="S100_SupportFileDiscoveryMetadata">
		<xs:annotation>
			<xs:documentation>Metadata about the individual support files in the exchange catalogue.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fileName" type="xs:anyURI" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name for the support file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="revisionStatus" type="S100_SupportFileRevisionStatus" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The purpose for which the support file has been issued</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="editionNumber" type="xs:positiveInteger" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The edition number of the support file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueDate" type="xs:date" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Date on which the data was made available by the data producer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supportFileSpecification" type="S100_SupportFileSpecification" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The specification used to create this file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataType" type="S100_SupportFileFormat" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The format of the support file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherDataTypeDescription" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Encoding format other than those listed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="comment" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Optional comment</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="compressionFlag" type="RestrictedBoolean" maxOccurs="1" minOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates if the resource is compressed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureReference" type="S100SE:S100_SE_DigitalSignatureReference_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Specifies the algorithm used to compute digitalSignatureValue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureValue" type="S100SE:S100_SE_DigitalSignature_PropertyType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Value derived from the digital signature. The value resulting from application of digitalSignatureReference. Implemented as the digital signature format specified in Part 15.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultLocale" type="lan:PT_Locale_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Default language and character set used in the support file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supportedResource" type="xs:string" maxOccurs="unbounded" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifier of the resource supported by this support file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="resourcePurpose" type="S100_ResourcePurpose" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The purpose of the supporting resource</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="S100_ExchangeCatalogueIdentifier" type="S100_ExchangeCatalogueIdentifier"/>
	<xs:complexType name="S100_ExchangeCatalogueIdentifier">
		<xs:annotation>
			<xs:documentation>An exchange catalogue contains the discovery metadata about the exchange datasets and support files</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="identifier" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>An identifier for an Exchange Catalogue.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateTime" type="xs:dateTime" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Creation date and time of the exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="S100_CataloguePointofContact">
		<xs:annotation>
			<xs:documentation>Contact details of the issuer of this exchange catalogue</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="organization" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The organization distributing this exchange catalogue. This could be an individual producer, value added reseller, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="phone" type="S100_Phone_Type" minOccurs="0" maxOccurs="1"/>
			<xs:element name="address" type="S100_Address_Type" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="S100_DatasetDiscoveryMetadata" type="S100_DatasetDiscoveryMetadata"/>
	<xs:complexType name="S100_DatasetDiscoveryMetadata">
		<xs:annotation>
			<xs:documentation>Metadata about the individual datasets in the exchange catalogue.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fileName" type="xs:anyURI" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Dataset file name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Short description giving the area or location covered by the dataset. e.g. a harbour or port name, between two named locations etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datasetID" type="MRNType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Dataset ID expressed as a Marine Resource Name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="compressionFlag" type="xs:boolean" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates if the resource is compressed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataProtection" type="RestrictedBoolean" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates if the data is encrypted</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="protectionScheme" type="S100_ProtectionScheme" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Specification of method used for data protection</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureReference" type="S100SE:S100_SE_DigitalSignatureReference_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Digital Signature of the file. Specifies the algorithm used to compute digitalSignatureValue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureValue" type="S100SE:S100_SE_DigitalSignature_PropertyType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Value derived from the digital signature. The value resulting from application of digitalSignatureReference. Implemented as the digital signature format specified in Part 15.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="copyright" type="RestrictedBoolean" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates if the dataset is copyrighted</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="classification" type="S100_ClassificationCode_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates the security classification of the dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purpose" type="S100_Purpose" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The purpose for which the dataset has been issued.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notForNavigation" type="RestrictedBoolean" maxOccurs="1" minOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates the dataset is not intended to be used for navigation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specificUsage" type="S100_Usage_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The use for which the dataset is intended. e.g. in the case of ENCs this would be a navigation purpose classification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="editionNumber" type="xs:positiveInteger" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The edition number of the dataset. When a data set is initially created, the edition number 1 is assigned to it. The edition number is increased by 1 at each new edition. Edition number remains the same for a re-issue.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="updateNumber" type="xs:nonNegativeInteger" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Update number assigned to the dataset and increased by one for each subsequent update. Update number 0 is assigned to a new dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="updateApplicationDate" type="gco:Date_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>This date is only used for the base cell files (i.e. new data sets, re-issue and new edition), not update cell files. All updates dated on or before this date must have been applied by the producer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueDate" type="xs:date" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Date on which the data was made available by the data producer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueTime" type="xs:time" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Time of day at which the data was made available by the data producer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="boundingBox" type="S100_GeographicBoundingBox_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The extent of the dataset limits.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="temporalExtent" type="S100_TemporalExtent" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specification of the temporal extent of the dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productSpecification" type="S100_ProductSpecification" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The product specification used to create this dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="producingAgency" type="cit:CI_Responsibility_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>ProducingAgency</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="producerCode" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The official IHO Producer Code from S-62</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="encodingFormat" type="S100_EncodingFormat_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The encoding format of the dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataCoverage" type="S100_DataCoverage" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Provides information about data coverages within the dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="comment" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Any additional information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultLocale" type="lan:PT_Locale_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Default language and character set used in the dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherLocale" type="lan:PT_Locale_PropertyType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Other languages and character sets used in the dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="metadataPointOfContact" type="cit:CI_Responsibility_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Point of contact for metadata</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="metadataDateStamp" type="xs:date" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Date stamp for metadata</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="replacedData" type="RestrictedBoolean" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If a data file is cancelled is it replaced by another data file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="navigationPurpose" maxOccurs="3" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Classification of intended navigation purpose (for Catalogue indexing purposes)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="resourceMaintenance" type="mmi:MD_MaintenanceInformation_PropertyType" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>S-100 restricts the ISO 19115-class to: (i) prohibit maintenanceScope, maintenanceNote, and contact attributes; (ii) define restrictions on maintenanceAndUpdateFrequency, maintenanceDate, and userDefinedMaintenanceFrequency attributes</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="S100_EncodingFormat">
		<xs:annotation>
			<xs:documentation>The encoding format.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ISO/IEC 8211">
				<xs:annotation>
					<xs:documentation>The ISO 8211 data format as defined in Part 10a</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GML">
				<xs:annotation>
					<xs:documentation>The GML data format as defined in Part 10b</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HDF5">
				<xs:annotation>
					<xs:documentation>The HDF5 data format as defined in Part 10c</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="undefined">
				<xs:annotation>
					<xs:documentation>The encoding is defined in the Product Specification</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="S100_SupportFileFormat">
		<xs:annotation>
			<xs:documentation>The format used for the support file.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ASCII"><xs:annotation><xs:documentation>UTF-8 text excluding control codes</xs:documentation></xs:annotation></xs:enumeration>
			<xs:enumeration value="JPEG2000"><xs:annotation><xs:documentation>JPEG2000 format</xs:documentation></xs:annotation></xs:enumeration>
			<xs:enumeration value="HTML"><xs:annotation><xs:documentation>Hypertext Markup Language</xs:documentation></xs:annotation></xs:enumeration>
			<xs:enumeration value="XML"><xs:annotation><xs:documentation>Extensible Markup Language</xs:documentation></xs:annotation></xs:enumeration>
			<xs:enumeration value="XSLT"><xs:annotation><xs:documentation>Extensible Stylesheet Language Transformations</xs:documentation></xs:annotation></xs:enumeration>
			<xs:enumeration value="VIDEO"><xs:annotation><xs:documentation>Representation of moving images in unspecified format</xs:documentation></xs:annotation></xs:enumeration>
			<xs:enumeration value="TIFF"><xs:annotation><xs:documentation>Tagged Image File Format</xs:documentation></xs:annotation></xs:enumeration>
			<xs:enumeration value="PDF/AorUA"><xs:annotation><xs:documentation>Portable Document Format</xs:documentation></xs:annotation></xs:enumeration>
			<xs:enumeration value="LUA"><xs:annotation><xs:documentation>Lua programming language</xs:documentation></xs:annotation></xs:enumeration>
			<xs:enumeration value="other"><xs:annotation><xs:documentation>Other format</xs:documentation></xs:annotation></xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="S100_CatalogueScope">
		<xs:annotation>
			<xs:documentation>The scope of the catalogue</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="featureCatalogue">
				<xs:annotation>
					<xs:documentation>S-100 feature catalogue</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="portrayalCatalogue">
				<xs:annotation>
					<xs:documentation>S-100 portrayal catalogue</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="interoperabilityCatalogue">
				<xs:annotation>
					<xs:documentation>S-100 interoperability information</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="S100_ProtectionScheme">
		<xs:annotation>
			<xs:documentation>Data protection schemes</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="S100p15">
				<xs:annotation>
					<xs:documentation>IHO S-100 Part 15</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="S100_ExchangeCatalogue" type="S100_ExchangeCatalogue"/>
	<xs:complexType name="S100_ExchangeCatalogue">
		<xs:annotation>
			<xs:documentation>Each exchange set has a single S-100_ExchangeCatalogue which contains meta information for the data and support files in the exchange set. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="identifier" type="S100_ExchangeCatalogueIdentifier" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Uniquely identifies this exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contact" type="S100_CataloguePointofContact" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Details about the issuer of this exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productSpecification" type="S100_ProductSpecification" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Details about the product specifications used for the datasets contained in the exchange catalogue. Conditional on all the datasets using the same product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultLocale" type="lan:PT_Locale_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Default language and character set used for all metadata records in this Exchange Catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherLocale" type="lan:PT_Locale_PropertyType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Other languages and character sets used for the localized metadata records in this Exchange Catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="exchangeCatalogueDescription" type="gco:CharacterString_PropertyType"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description of what the exchange catalogue contains</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="exchangeCatalogueComment" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Any additional Information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="certificates" type="S100SE:S100_SE_CertificateContainerType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataServerIdentifier" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the data server for the permit</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datasetDiscoveryMetadata">
				<xs:annotation>
					<xs:documentation>Container for S100_DatasetDiscoveryMetadata elements</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="S100_DatasetDiscoveryMetadata" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Discovery metadata for single dataset. Constraints should be in a separate file for reusability and potential customization.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="supportFileDiscoveryMetadata">
				<xs:annotation>
					<xs:documentation>Exchange Catalogues may include or reference discovery metadata for the support files in the Exchange Set</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="S100_SupportFileDiscoveryMetadata" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="catalogueDiscoveryMetadata">
				<xs:annotation>
					<xs:documentation>Metadata for Catalogue</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="S100_CatalogueDiscoveryMetadata" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

