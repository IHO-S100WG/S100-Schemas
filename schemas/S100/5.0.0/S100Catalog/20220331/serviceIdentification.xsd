  elementFormDefault="qualified"
  targetNamespace="http://www.iho.int/s100/md/srv/5.0"
  version="5.0.0-20220331">
  <annotation>
    <documentation>S-100 schema for extensions to identification information for services</documentation>
  </annotation>

