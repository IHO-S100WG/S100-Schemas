  <import namespace="http://standards.iso.org/iso/19115/-3/gco/1.0" schemaLocation="https://schemas.isotc211.org/19115/-3/gco/1.0/gco.xsd"/>
  <import namespace="http://standards.iso.org/iso/19115/-3/gmw/1.0" schemaLocation="https://schemas.isotc211.org/19115/-3/gmw/1.0/gmw.xsd"/>
  <import namespace="http://standards.iso.org/iso/19115/-3/lan/1.0" schemaLocation="https://schemas.isotc211.org/19115/-3/lan/1.0/lan.xsd"/>
  <import namespace="http://standards.iso.org/iso/19115/-3/mcc/1.0" schemaLocation="https://schemas.isotc211.org/19115/-3/mcc/1.0/mcc.xsd"/>
  <import namespace="http://standards.iso.org/iso/19115/-3/mri/1.0" schemaLocation="https://schemas.isotc211.org/19115/-3/mri/1.0/mri.xsd"/>
  <import namespace="http://standards.iso.org/iso/19115/-3/mrd/1.0" schemaLocation="https://schemas.isotc211.org/19115/-3/mrd/1.0/mrd.xsd"/>
  <import namespace="http://standards.iso.org/iso/19115/-3/srv/2.0" schemaLocation="https://schemas.isotc211.org/19115/-3/srv/2.0/srv.xsd"/>
  <import namespace="http://standards.iso.org/iso/19115/-3/mds/2.0" schemaLocation="https://schemas.isotc211.org/19115/-3/mds/2.0/mds.xsd"/>

  <element name="S100_SV_ServiceIdentification" substitutionGroup="mri:AbstractMD_Identification" type="s100srv:S100_SV_ServiceIdentification_Type">
    <annotation>
      <documentation>extended information required to identify a service in S-100</documentation>
    </annotation>
  </element>

  <complexType name="S100_SV_ServiceIdentification_Type">
    <complexContent>
      <extension base="srv:SV_ServiceIdentification_Type">
        <sequence>
          <!-- S-100 specific service metadata elements can be added here -->
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  
  <complexType name="S100_SV_ServiceIdentification_PropertyType">
    <sequence minOccurs="0">
      <element ref="s100srv:S100_SV_ServiceIdentification"/>
    </sequence>
    <attributeGroup ref="gco:ObjectReference"/>
    <attribute ref="gco:nilReason"/>
  </complexType>

</schema>
