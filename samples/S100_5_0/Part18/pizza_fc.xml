      <S100FC:alias></S100FC:alias>
      <S100FC:valueType>real</S100FC:valueType>
    </S100FC:S100_FC_SimpleAttribute>
    <S100FC:S100_FC_SimpleAttribute>
      <S100FC:name>Name</S100FC:name>
      <S100FC:definition>The name of the pizza.</S100FC:definition>
      <S100FC:code>name</S100FC:code>
      <S100FC:valueType>text</S100FC:valueType>
    </S100FC:S100_FC_SimpleAttribute>
  </S100FC:S100_FC_SimpleAttributes>
  <S100FC:S100_FC_FeatureTypes>
    <S100FC:S100_FC_FeatureType isAbstract="false">
      <S100FC:name>Pizza</S100FC:name>
      <S100FC:definition>Pizza is a dish of Italian origin consisting of a usually round, flat base of leavened wheat-based dough topped with tomatoes, cheese, and often various other ingredients (such as anchovies, mushrooms, onions, olives, pineapple, meat, etc.), which is then baked at a high temperature, traditionally in a wood-fired oven.</S100FC:definition>
      <S100FC:code>pizza</S100FC:code>
      <S100FC:definitionReference>
        <S100FC:sourceIdentifier>https://en.wikipedia.org/wiki/Pizza</S100FC:sourceIdentifier>
        <S100FC:definitionSource ref="Wikipedia"/>
      </S100FC:definitionReference>
      <S100FC:attributeBinding>
        <S100FC:multiplicity>
          <S100Base:lower>1</S100Base:lower>
          <S100Base:upper>4</S100Base:upper>
        </S100FC:multiplicity>
        <S100FC:attribute ref="categoryOfTopping"/>
      </S100FC:attributeBinding>
      <S100FC:attributeBinding>
        <S100FC:multiplicity>
          <S100Base:lower>1</S100Base:lower>
          <S100Base:upper>1</S100Base:upper>
        </S100FC:multiplicity>
        <S100FC:attribute ref="size"/>
      </S100FC:attributeBinding>
      <S100FC:attributeBinding>
        <S100FC:multiplicity>
          <S100Base:lower>1</S100Base:lower>
          <S100Base:upper>1</S100Base:upper>
        </S100FC:multiplicity>
        <S100FC:attribute ref="name"/>
      </S100FC:attributeBinding>
      <S100FC:featureUseType>cartographic</S100FC:featureUseType>
      <S100FC:permittedPrimitives>noGeometry</S100FC:permittedPrimitives>
    </S100FC:S100_FC_FeatureType>
  </S100FC:S100_FC_FeatureTypes>
