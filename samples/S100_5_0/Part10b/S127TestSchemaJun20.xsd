  <xs:simpleType name="categoryOfCargoLabel">
    <xs:annotation>
      <xs:documentation>Category of cargo: Classification of the different types of cargo that a ship may be carrying.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="bulk">
        <xs:annotation>
          <xs:documentation>1: Normally dry cargo which is transported to and from the vessel on conveyors or grabs </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="container">
        <xs:annotation>
          <xs:documentation>2: One of a number of standard sized cargo carrying units, secured using standard corner attachments and bars </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="general">
        <xs:annotation>
          <xs:documentation>3: Break bulk cargo normally loaded by crane</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="liquid">
        <xs:annotation>
          <xs:documentation>4: Any cargo loaded by pipeline</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="passenger">
        <xs:annotation>
          <xs:documentation>5: A fee paying traveller</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="livestock">
        <xs:annotation>
          <xs:documentation>6: Live animals carried in bulk</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="dangerous or hazardous">
        <xs:annotation>
          <xs:documentation>7: Dangerous or hazardous cargo as described by the IMO International Maritime Dangerous Goods code</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="heavy lift">
        <xs:annotation>
          <xs:documentation>8: Indivisible heavy items of weight generally over 100 tons, and width or height greater than 100 meters.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ballast">
        <xs:annotation>
          <xs:documentation>9: Material carried by a ship to ensure its stability.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
