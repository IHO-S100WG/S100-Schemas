  <xs:simpleType name="categoryOfMaritimeBroadcastLabel">
    <xs:annotation>
      <xs:documentation>Category of maritime broadcast: Classification of maritime broadcast based on the nature of information conveyed.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="navigational warning">
        <xs:annotation>
          <xs:documentation>1: message containing urgent information relevant to safe navigation broadcast to ships in accordance with the provisions of the International Convention for the Safety of Life at Sea, 1974, as </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="meteorological warning">
        <xs:annotation>
          <xs:documentation>2: warning of adverse weather conditions</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ice report">
        <xs:annotation>
          <xs:documentation>3: report of the ice situation and restrictions to shipping</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SAR information">
        <xs:annotation>
          <xs:documentation>4: broadcast message with information about an ongoing SAR operation</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="pirate attack warning">
        <xs:annotation>
          <xs:documentation>5: warning of possible attack by pirates</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="meteorological forecast">
        <xs:annotation>
          <xs:documentation>6: broadcast message containing meteorological forecast</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="pilot service message">
        <xs:annotation>
          <xs:documentation>7: broadcast message about pilot service</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AIS information">
        <xs:annotation>
          <xs:documentation>8: broadcast message about AIS information</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="LORAN message">
        <xs:annotation>
          <xs:documentation>9: broadcast message about the LORAN service</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SATNAV message">
        <xs:annotation>
          <xs:documentation>10: broadcast message about Satellite Navigation service</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="gale warning">
        <xs:annotation>
          <xs:documentation>11: warning of winds of Beaufort force 8 or 9</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="storm warning">
        <xs:annotation>
          <xs:documentation>12: warning of winds of Beaufort force 10 or over</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="tropical revolving storm warning">
        <xs:annotation>
          <xs:documentation>13: warning of hurricanes in the North Atlantic and eastern North Pacific, typhoons in the Western Pacific, cyclones in the Indian Ocean and cyclones of similar nature in other regions</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NAVAREA warning">
        <xs:annotation>
          <xs:documentation>14: navigational warning or in-force bulletin promulgated as part of a numbered series by a NAVAREA coordinator (Maritime Safety Information Manual 2009)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="coastal warning">
        <xs:annotation>
          <xs:documentation>15: navigational warning promulgated as part of a numbered series by a National coordinator (Maritime Safety Information Manual 2009)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="local warning">
        <xs:annotation>
          <xs:documentation>16: warning which covers inshore waters, often within the limits of jurisdiction of a harbour or port authority (Maritime Safety Information Manual 2009)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="low water level warning/negative tidal surge">
        <xs:annotation>
          <xs:documentation>17: warning of actual or expected low water level</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="icing warning">
        <xs:annotation>
          <xs:documentation>18: warning of accretion of ice on ships</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="tsunami broadcast">
        <xs:annotation>
          <xs:documentation>19: broadcasts about tsunamis, including watches, advisories, and other types of messages relating to tsunamis or potential tsunamis</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
