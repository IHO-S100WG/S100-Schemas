        <xs:annotation>
          <xs:documentation>13: Frequency in a frequency range between 3 and 30 MHz used for digital traffic</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Very High Frequency (VHF) digital traffic">
